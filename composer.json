{
  "name": "nipwaayoni/elastic-apm-laravel",
  "keywords": [
    "laravel",
    "elastic",
    "APM",
    "Lumen",
    "monitoring",
    "debug"
  ],
  "description": "A package to integrate Elastic APM into Laravel",
  "license": "MIT",
  "require": {
    "php": ">= 7.2",
    "illuminate/database": ">= 6.0 < 8",
    "illuminate/http": ">= 6.0 < 8",
    "illuminate/routing": ">= 6.0 < 8",
    "illuminate/support": ">= 6.0 < 8",
    "nipwaayoni/elastic-apm-php-agent": "7.1.0-beta",
    "ramsey/uuid": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Nipwaayoni\\ElasticApmLaravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nipwaayoni\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nipwaayoni\\ElasticApmLaravel\\Providers\\ElasticApmServiceProvider"
      ],
      "aliases": {
        "ElasticApm": "Nipwaayoni\\ElasticApmLaravel\\Facades\\ElasticApm"
      }
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "authors" : [
    {
      "name" : "Dirk Tepe",
      "email" : "dstepe@gmail.com",
      "homepage" : "https://github.com/dstepe"
    }
  ],
  "scripts": {
    "fix": "\"./tools/php-cs-fixer\" --config=config/php-cs-fixer.php fix src/",
    "ci:tests": "\"tools/phpunit\" tests/",
    "ci:fixer": "\"./tools/php-cs-fixer\" --config=config/php-cs-fixer.php fix --dry-run -v --show-progress=dots --diff-format=udiff src/"
  }
}
